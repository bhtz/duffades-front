{
  "name": "com.duffade.front",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "http-server ./www -s",
    "serve": "ng serve --port 3000",
    "test": "ng test",
    "lint": "ng lint",
    "build": "ng build",
    "build:int": "ng build --int --aot && npm run precache",
    "build:prod": "ng build --prod --aot && npm run precache",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "doc": "compodoc src -d docs -p ./src/tsconfig.json -a ./src/assets -s -o -r 3001",
    "precache": "sw-precache --verbose --config=sw-precache-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.3.1",
    "@angular/compiler": "2.3.1",
    "@angular/core": "2.3.1",
    "@angular/forms": "2.3.1",
    "@angular/http": "2.3.1",
    "@angular/platform-browser": "2.3.1",
    "@angular/platform-browser-dynamic": "2.3.1",
    "@ionic/storage": "1.1.7",
    "core-js": "2.4.1",
    "font-awesome": "^4.7.0",
    "http-server": "^0.9.0",
    "ionic-angular": "^2.3.0",
    "ionic-native": "^2.9.0",
    "ionicons": "3.0.0",
    "parse": "^1.10.0",
    "rxjs": "5.0.1",
    "sw-toolbox": "3.4.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.32.3",
    "@angular/compiler-cli": "2.3.1",
    "@ionic/cli-plugin-ionic-angular": "1.4.1",
    "@types/gulp": "^3.8.32",
    "@types/jasmine": "2.5.38",
    "@types/jquery": "^2.0.40",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "compodoc": "0.0.41",
    "gulp": "^3.9.1",
    "ionic": "3.7.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "sw-precache": "^5.1.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  },
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-statusbar",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": []
